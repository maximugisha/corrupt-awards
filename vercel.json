{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "build": {
      "env": {
        "PRISMA_GENERATE": "true"
      },
      "commands": {
        "build": "prisma generate && next build",
        "postbuild": "prisma migrate deploy"
      }
    }
  }
  